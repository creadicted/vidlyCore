@model VidlyCore.ViewModel.RandomMovieViewModel

@{
    ViewData["Title"] = "Random Movie";
}

@{
    var className = Model.Customers.Count > 5 ? "popular" : null;
}

<h1 class="@className">@Model.Movie.Name</h1>
<ul>
    @foreach (var customer in Model.Customers)
    {
        <li>@customer.Name</li>
    }
</ul>